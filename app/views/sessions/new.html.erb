<%= javascript_include_tag 'application' %>
<!--above tag works but errors in console-->
  <h1>Please Sign In</h1>

  <%= render "shared/errors" %>


<body>
<div class="row">
  <form class="col s12" action="<%= session_url %>" method="post">
    <div class="row">
      <input
        type="hidden"
        name="authenticity_token"
        value="<%= form_authenticity_token %>">

      <div class="input-field col s6">
        <i class="material-icons prefix">perm_identity</i>
        <input type="text" name="user[username]" id="user_username">
        <label for="user_username" class="active">Username</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix">lock</i>
        <input id="password" type="password" class="validate" name="user[password]">
        <label for="password" class="active">Password</label>
      </div>
    </div>
    <br>
    <div class="submit-field col s5 offset-s1">
        <button class="btn primary waves-effect waves-light" type="submit" name="action">Log In<i class="mdi-action-lock-open right"></i>
        </button>
    </div>

  </form>
</div>

